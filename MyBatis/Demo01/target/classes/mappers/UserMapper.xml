<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.mapper.UserMapper">

    <insert id="insertUser">
        insert into t_user values(null, 'admin', '123123', 21, 'male', 'sjiang23@jhu.edu')
    </insert>

    <update id="updateUser">
        update t_user set username = 'Jason' where id = 4
    </update>

    <delete id="deleteUser">
        delete from t_user where id = 6
    </delete>

    <select id="getUserById" resultType="com.mybatis.pojo.User">
        select * from t_user where id = 3
    </select>

    <select id="getAllUsers" resultType="com.mybatis.pojo.User">
        select * from t_user
    </select>

</mapper>